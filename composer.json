{
    "name": "gabrielbull/gearman-handler",
    "type": "library",
    "description": "PHP Daemon for handling and managing Gearman Workers",
    "keywords": ["Gearman", "Workers", "Daemon"],
    "homepage": "https://github.com/gabrielbull/php-gearman-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Bull",
            "email": "me@gabrielbull.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "2.*",
        "react/event-loop": "0.4.*",
        "psr/log": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "suggest": {
        "ext-gearman": "This library needs Gearman"
    },
    "autoload": {
        "psr-4": {
            "GearmanHandler\\": "src/GearmanHandler"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GearmanHandler\\Tests\\": ["tests/GearmanHandler/Tests", "tests/GearmanHandler/Tests/_includes"]
        }
    },
    "bin": ["bin/gearman", "bin/gearman.php"]
}